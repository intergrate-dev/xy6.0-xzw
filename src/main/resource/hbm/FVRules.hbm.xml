<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.founder.e5.dom.FVRules" table="DOM_FVRULES">
		<composite-id>
			<key-property name="fvID" column="FVID" type="integer"/>
			<key-property name="ruleID" column="RULEID" type="integer"/>
		</composite-id>
		<property
			name="subIndex"
			column="SUBINDEX"
			type="integer"
			not-null="false"
		 />
	</class>
</hibernate-mapping>
