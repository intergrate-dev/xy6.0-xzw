<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.founder.e5.sys.org">
	<class name="com.founder.e5.sys.org.UserRole" table="FSYS_ROLEUSERRELATIONS">
		<composite-id>
			<key-property
				column="ROLEID"
				name="RoleID"
				type="integer"
			 />
			<key-property
				column="USERID"
				name="UserID"
				type="integer"
			 />
		</composite-id>
		<property
			column="TIMEVALUE"
			length="22"
			name="TimeValue"
			not-null="false"
			type="integer"
		 />
		<property
			column="ROLENAME"
			length="254"
			name="RoleName"
			not-null="false"
			type="string"
		 />
		<property
			column="ENDTIME"
			length="8"
			name="EndTime"
			not-null="false"
			type="string"
		 />
		<property
			column="ENDDATE"
			length="10"
			name="EndDate"
			not-null="false"
			type="string"
		 />
		<property
			column="STARTTIME"
			length="8"
			name="StartTime"
			not-null="false"
			type="string"
		 />
		<property
			column="STARTDATE"
			length="10"
			name="StartDate"
			not-null="false"
			type="string"
		 />
		<property
			column="USERCODE"
			length="254"
			name="UserCode"
			not-null="false"
			type="string"
		 />
		<property
			column="TIMETYPE"
			length="22"
			name="TimeType"
			not-null="false"
			type="integer"
		 />
	</class>
</hibernate-mapping>
