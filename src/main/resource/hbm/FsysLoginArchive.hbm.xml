<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.founder.e5.sys">
	<class name="com.founder.e5.sys.LoginArchive" table="FSYS_LOGINARCHIVE">
		<id
			column="LOGINID"
			name="loginID"
			type="integer"
		>
		<generator class="assigned" />
		</id>
		<property
			column="NROLEID"
			name="roleID"
			not-null="true"
			type="integer"
		 />
		<property
			column="NUSERID"
			name="userID"
			not-null="true"
			type="integer"
		 />
		<property
			column="LOGINTIME"
			length="29"
			name="loginTime"
			not-null="true"
			type="string"
		 />
		 <property
			column="LASTACCESSTIME"
			length="29"
			name="lastAccessTime"
			not-null="true"
			type="string"
		 />
		<property
			column="STRHOSTNAME"
			length="20"
			name="hostName"
			not-null="true"
			type="string"
		 />
		<property
			column="SERVERNAME"
			length="20"
			name="serverName"
			not-null="false"
			type="string"
		 />
		<property
			column="PORTALID"
			length="3"
			name="portalID"
			not-null="false"
			type="integer"
		 />
		<property
			column="STRUSERCODE"
			length="254"
			name="userCode"
			not-null="true"
			type="string"
		 />
		<property
			column="APPID"
			length="3"
			name="appID"
			not-null="false"
			type="integer"
		 />
		<property
			column="normalexit"
			length="3"
			name="normalExit"
			not-null="false"
			type="integer"
		 />
	</class>
</hibernate-mapping>
